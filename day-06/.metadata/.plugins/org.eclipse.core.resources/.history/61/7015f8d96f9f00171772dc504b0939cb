class StaticCode {
    static {
        System.out.println("This is a Static Code");
    }
}

class StaticCodeDemo {
	static {
		System.out.println("a");
	}
    public static void main(String[] args) {
    	new StaticCode()
    	new StaticCode()
        System.out.println("Hello World");
    }
    static {
		System.out.println("c");
	}
}